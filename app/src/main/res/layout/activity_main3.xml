<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/image_7"
    tools:context=".MainActivity3">


    <LinearLayout
        android:layout_width="250dp"
        android:layout_height="430dp"
        android:layout_marginTop="108dp"
        android:orientation="vertical"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" >

        <ImageView
            android:id="@+id/imageView18"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginVertical="0.544dp"
            android:layout_marginTop="42dp"
            app:layout_constraintTop_toBottomOf="@+id/textView"
            app:srcCompat="@drawable/component_17"
            android:contentDescription="@null"/>

        <ImageView
            android:id="@+id/imageView19"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView18"
            app:srcCompat="@drawable/component_18"
            tools:layout_editor_absoluteX="81dp"
            android:contentDescription="@null"/>

        <ImageView
        android:id="@+id/imageView24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        app:layout_constraintTop_toBottomOf="@+id/imageView19"
        app:srcCompat="@drawable/component_19"
        android:contentDescription="@null"/>

        <ImageView
            android:id="@+id/imageView22"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="32dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView24"
            app:srcCompat="@drawable/component_20"
            android:contentDescription="@null"/>

        <ImageView
            android:id="@+id/imageView20"
            android:layout_width="250dp"
            android:layout_height="45dp"
            android:layout_marginTop="32dp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/imageView22"
            app:srcCompat="@drawable/component_21"
            android:contentDescription="@null"/>

        <ImageView
            android:id="@+id/imageView21"
            android:layout_width="250dp"
            android:layout_height="45dp"
            android:layout_marginTop="32dp"
            app:layout_constraintTop_toBottomOf="@+id/imageView20"
            app:srcCompat="@drawable/component_23"
            tools:layout_editor_absoluteX="162dp"
            android:contentDescription="@null"/>

    </LinearLayout>


    <TextView
        android:id="@+id/textView"
        android:layout_width="341dp"
        android:layout_height="63dp"
        android:layout_marginTop="88dp"
        android:gravity="center"
        android:scrollbarSize="10dp"
        android:text="@string/Theme_1"
        android:textColor="#FFFFFF"
        android:textSize="22sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="48dp"
        android:layout_height="48dp"
        android:layout_marginTop="16dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.044"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_action_name"
        android:contentDescription="@null"/>


</androidx.constraintlayout.widget.ConstraintLayout>
